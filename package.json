{
  "name": "ufs-web-client",
  "version": "1.0.0",
  "private": true,
  "description": "Unified Financial Services Platform - Web Frontend",
  "keywords": ["fintech", "financial-services", "react", "nextjs", "typescript"],
  "author": "UFS Development Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "playwright": "playwright test",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "next": "14.2.3",
    "typescript": "5.3.3",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.15",
    "@mui/icons-material": "^5.15.15",
    "@mui/system": "^5.15.14",
    "@reduxjs/toolkit": "^2.2.3",
    "react-redux": "9.1.1",
    "axios": "^1.6.8",
    "zod": "3.22.4",
    "next-auth": "4.24.7",
    "react-hook-form": "7.51.3",
    "@hookform/resolvers": "3.3.4",
    "framer-motion": "11.1.7",
    "chart.js": "4.4.2",
    "react-chartjs-2": "5.2.0",
    "date-fns": "^3.6.0",
    "clsx": "2.1.0",
    "tailwind-merge": "2.2.1",
    "@tanstack/react-query": "4.29.19",
    "class-variance-authority": "0.7.0",
    "@radix-ui/react-dialog": "1.0.5",
    "lucide-react": "0.378.0",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-tabs": "1.0.4",
    "recharts": "2.12.2",
    "react-router-dom": "6.22.3",
    "@sentry/nextjs": "7.100.1",
    "styled-components": "5.3.11",
    "react-dropzone": "14.2.3",
    "@tanstack/react-table": "8.10.7",
    "react-icons": "4.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/jest": "^29.5.12",
    "@types/styled-components": "^5.1.34",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "@typescript-eslint/parser": "5.62.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "autoprefixer": "^10.4.1",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "axios-mock-adapter": "1.22.0",
    "cypress": "^13.8.1",
    "@playwright/test": "^1.43.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "webpack": "^5.89.0",
    "vite": "^5.0.0",
    "@jest/types": "^29.6.3",
    "@next/bundle-analyzer": "^14.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testPathIgnorePatterns": ["<rootDir>/.next/", "<rootDir>/node_modules/"],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": ["ts-jest", {
        "tsconfig": "tsconfig.json"
      }]
    },
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/company/ufs-web-client.git"
  },
  "bugs": {
    "url": "https://github.com/company/ufs-web-client/issues"
  },
  "homepage": "https://github.com/company/ufs-web-client#readme"
}